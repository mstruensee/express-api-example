		"express": "4.17.1",
		"morgan": "1.10.0",
		"mysql": "2.18.1"
	},
	"devDependencies": {
		"@babel/core": "7.13.8",
		"@babel/node": "7.13.0",
		"@babel/plugin-proposal-class-properties": "7.13.0",
		"@babel/plugin-transform-modules-commonjs": "7.13.8",
		"babel-loader": "8.2.2",
		"clean-webpack-plugin": "3.0.0",
		"cross-env": "7.0.3",
		"jest": "26.6.3",
		"nodemon": "2.0.7",
		"npm-check-updates": "11.1.10",
		"supertest": "6.1.3",
